    <script>
        document.querySelectorAll('.testimonial-card button').forEach(button => {
            button.addEventListener('click', function() {
                const card = this.closest('.testimonial-card');
                const content = card.querySelector('p');
                content.classList.toggle('line-clamp-4');
                this.textContent = content.classList.contains('line-clamp-4') ? 'Read More' : 'Read Less';
            });
        });
    </script>
